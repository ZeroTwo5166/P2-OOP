@model IEnumerable<Category>
@{
    ViewData["Title"] = "Index";
}

<h1>Index Category</h1>


<table class="table table-bordered table-stripped" style="width:100%">
    <thead>
        <tr>
            <th>
                Id
            </th>
            <th>
                Film Titel
            </th>
            <th>
                Instruktør
            </th>
            <th>
                Årstal
            </th>
            <th>
                ...
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach(var obj in Model)
        {
            <tr>
                <td width="5%">
                    @obj.Id
                </td>
                <td width="25%">
                    @obj.Film_Titel
                </td>
                <td width="25%">
                    @obj.Instruktør
                </td>
                <td width="5%">
                    @obj.Årstal
                </td>
                <td width="40%">
                    .
                </td>
            </tr>
        }
    </tbody>
</table>

